{
  "name": "dividela2",
  "version": "1.0.0",
  "description": "A simple, privacy-focused expense tracking app designed specifically for couples who want to manage shared expenses effortlessly.",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:web": "expo export --platform web",
    "deploy": "npm run build:web && firebase deploy --only hosting",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:performance": "jest --config functions/__tests__/performance/jest.config.js",
    "test:ml-accuracy": "jest functions/__tests__/ml/accuracyMonitoring.test.js",
    "test:e2e": "detox test",
    "test:e2e:build": "detox build",
    "test:e2e:ios": "detox test --configuration ios.sim.debug",
    "test:e2e:android": "detox test --configuration android.emu.debug"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/miguelangat/dividela2.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/miguelangat/dividela2/issues"
  },
  "homepage": "https://github.com/miguelangat/dividela2#readme",
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.24.0",
    "@react-native-community/datetimepicker": "^8.5.1",
    "@react-native-community/slider": "^5.1.1",
    "@react-native-google-signin/google-signin": "^16.0.0",
    "@react-navigation/bottom-tabs": "^7.7.3",
    "@react-navigation/native": "^7.1.19",
    "@react-navigation/stack": "^7.6.2",
    "buffer": "^6.0.3",
    "expo": "^54.0.21",
    "expo-clipboard": "^8.0.7",
    "expo-constants": "~18.0.11",
    "expo-device": "~8.0.10",
    "expo-document-picker": "^14.0.7",
    "expo-file-system": "^19.0.19",
    "expo-image-manipulator": "^14.0.7",
    "expo-image-picker": "^17.0.8",
    "expo-linear-gradient": "^15.0.7",
    "expo-localization": "^17.0.7",
    "expo-notifications": "~0.32.14",
    "expo-status-bar": "^3.0.8",
    "firebase": "^12.5.0",
    "i18next": "^25.6.2",
    "papaparse": "^5.5.3",
    "pdf-parse": "^2.4.5",
    "pdfjs-dist": "^5.4.394",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-i18next": "^16.3.3",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "^2.29.1",
    "react-native-paper": "^5.14.5",
    "react-native-purchases": "^9.6.6",
    "react-native-reanimated": "^4.1.3",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-screens": "4.16.0",
    "react-native-vector-icons": "^10.3.0",
    "react-native-web": "^0.21.0",
    "react-native-worklets": "^0.7.0",
    "rn-emoji-keyboard": "^1.7.0",
    "string-similarity": "^4.0.4"
  },
  "devDependencies": {
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.4.3",
    "detox": "^20.28.3",
    "jest": "^29.7.0",
    "jest-circus": "^29.7.0",
    "jest-expo": "^52.0.3",
    "prettier": "^3.6.2",
    "react-test-renderer": "19.1.0"
  }
